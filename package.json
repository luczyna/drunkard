  "main": "gulpfile.js",
  "dependencies": {
    "bower": "^1.4.1",
    "express": "^4.12.4",
    "chalk": "^1.0.0",
    "dateformat": "^1.0.11",
    "gulp": "^3.9.0",
    "gulp-angular-templatecache": "^1.6.0",
    "gulp-clean": "^0.3.1",
    "gulp-concat": "^2.5.2",
    "gulp-develop-server": "^0.4.3",
    "gulp-jshint": "^1.11.0",
    "gulp-load-plugins": "^1.0.0-rc.1",
    "gulp-inject": "^1.3.1",
    "gulp-load-utils": "^0.0.4",
    "gulp-minify-css": "^1.1.6",
    "gulp-ng-annotate": "^1.0.0",
    "gulp-plumber": "^1.0.1",
    "gulp-sass": "^2.0.1",
    "gulp-sequence": "^0.3.2",
    "gulp-uglify": "^1.2.0",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-template": "^3.0.0",
    "minimist": "^1.1.1",
    "jshint-stylish": "^2.0.0"
  },
  "devDependencies": {},
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js"
  },
  "keywords": [
    "drunk",
    "angular",
    "test"
  ],
  "author": "tropvache",
  "license": "MIT",
  "paths": {
    "source": {
      "base": "./src/",
      "sass": [
        "./src/sass/**/*.scss",
        "./src/app/**/*.scss",
        "./src/common/**/*.scss"
      ],
      "js": [
        "./src/app/**/*.js",
        "./src/common/**/*.js"
      ],
      "html": [
        "./src/index.html",
        "./src/app/**/*.html",
        "./src/common/**/*.html"
      ],
      "assets": [
        "./src/assets/**/*"
      ]
    },
    "dest": {
      "base": "./build/",
      "css": "./build/content/",
      "js": "./build/js/",
      "vendor": "build/vendor",
      "images": "./build/content/images"
    },
    "production": "./prod/"
  },
  "vendor": [
    "vendor/jquery/dist/jquery.min.js",
    "vendor/angular/angular.min.js",
    "vendor/angular-ui-router/release/angular-ui-router.js",
    "vendor/angular-animate/angular-animate.js",
    "vendor/angular-touch/angular-touch.js"
  ],
  "includejs": [
    "vendor/jquery.min.js",
    "vendor/angular.min.js",
    "vendor/angular-ui-router.js",
    "vendor/angular-animate.js",
    "vendor/angular-touch.js",
    "templates.js",
    "js/drunkard.js"
  ]
}
